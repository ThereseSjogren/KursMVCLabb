
@{
    ViewBag.Title = "UploadPicture";
}

<h2>UploadPicture</h2>
@using (Html.BeginForm(
                        "UploadPicture",
                        "Gallery",
                        FormMethod.Post,
                        new { enctype = "multipart/form-data" }))
{
    <input type="file" name="file" id="file" />
    <input type="submit" name="Create"/>
}

